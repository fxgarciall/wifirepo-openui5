<mvc:View
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    controllerName="WiFiRepo.controller.inicial">
	<App>
		<pages>
      <Page id="searchWiFi" title="{i18n>titulo}">
    		<headerContent>
    		</headerContent>
    		<content>
    			<mvc:XMLView viewName="WiFiRepo.view.WiFiList"/>
          <l:VerticalLayout
            class="sapUiContentPadding"
            id="oStripContent"
            width="100%"/>
        </content>
    		<footer>
    			<Bar>
    				<contentRight>
    					<Button id="new" text="New" press="handleNewPress" />
    				</contentRight>
    			</Bar>
    		</footer>
    	</Page>
		</pages>
	</App>
</mvc:View>
