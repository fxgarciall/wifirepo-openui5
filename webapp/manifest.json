{
	"_version": "1.0.0",
	"sap.app": {
		"_version": "1.0.0",
		"id": "WiFiRepo",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {

		}
	},
	"sap.ui": {
		"_version": "1.0.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"_version": "1.0.0",
		"rootView": {
			"viewName": "WiFiRepo.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"openui5.googlemaps": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"globalWiFiList": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/data.json"
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "WiFiRepo.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "WiFiRepo.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "appHome",
					"target": "home"
				},
				{
					"pattern": "detail/{wifiPath}",
					"name": "detail",
					"target": "detail"
				},
				{
					"pattern": "new",
					"name": "new",
					"target": "new"
				}
			],
			"targets": {
				"home": {
					"viewName": "inicial",
					"viewLevel": 0
				},
				"detail": {
					"viewName": "Detail",
					"viewLevel": 1
				},
				"notFound": {
					"viewName": "NotFound"
				},
				"errorOdata": {
					"viewName": "ErrorOdata"
				},
				"new": {
					"viewName": "New"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}
